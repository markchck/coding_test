# https://www.acmicpc.net/problem/1978

n = int(input())
numbers=map(int, input().split(" "))

def judge(numbers):
  sosu =0 
  not_sosu = 0
  for number in numbers:
    if number>1:
      for n in range(2, number):
        if(number%n ==0):
          not_sosu = not_sosu +1
      if not_sosu == 0:
        sosu = sosu +1
  print(sosu)
judge(numbers)